{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Server-Side",
        "type": "node",
        "request": "launch",
        "program": "${workspaceFolder}\node_modules/next/dist/bin/next",
        "args": ["dev"],
        "cwd": "${workspaceFolder}",
        "runtimeArgs": ["--inspect"]
      },
      {
        "name": "Client-Side",
        "type": "chrome",
        "request": "launch",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceFolder}"
      }
    ]
  }
  